generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Entry {
  id        Int      @id @default(autoincrement())
  date      String
  hours     Float
  type      String
  note      String?
  requiresMemo Boolean  @default(false)
  memoDone     Boolean  @default(false)
  createdAt DateTime @default(now())
}
